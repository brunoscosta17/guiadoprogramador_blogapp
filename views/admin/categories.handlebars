<h2>Categories List</h2>
<hr>
<a href="/admin/categories/add"><button class="btn btn-secondary">New Category</button></a>

{{#each categories}}
    <div class="card card-body mt-4">
        <h4>{{name}}</h4>
        <small>{{slug}}</small>
        <small>{{createdAt}}</small>
    </div>
    {{else}}
{{/each}}